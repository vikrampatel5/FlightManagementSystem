<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.fms.Flight" table="flight">
        <id column="id" name="id" type="java.lang.Integer" />
        <property column="ModelName" name="modelName" type="java.lang.String" />
        <property column="Source" name="source" type="java.lang.String" />
        <property column="Destination" name="destination" type="java.lang.String" />
        <property column="Fare" name="fare" type="java.lang.Integer" />
        <property column="Days" name="days" type="java.lang.String" />
    </class>
</hibernate-mapping>